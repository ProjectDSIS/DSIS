<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LibActivity" >

    <WebView
        android:id="@+id/wv_lib"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />

    <ProgressBar
        android:id="@+id/bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="invisible" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <!-- 1번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="100"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/imageButton1"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:background="@drawable/top_lib"
                />
        </LinearLayout>

        <!-- 2번째 -->

       <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="65"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_st4"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="30"
                android:background="#00ff00ff"
                android:onClick="ck_st4" />



        </LinearLayout>
        
        <!-- 3번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="0"
            android:orientation="horizontal" >

            <Button
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="11"
                android:background="#00ee0000" />
        </LinearLayout>

        <!-- 4번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="65"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_st5a"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="30"
                android:background="#00ff00ff"
                android:onClick="ck_st5a" />

        </LinearLayout>
        
        <!-- 5번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="0"
            android:orientation="horizontal" >

            <Button
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="11"
                android:background="#00ee0000" />
        </LinearLayout>

        <!-- 6번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="65"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_st5b"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="30"
                android:background="#00ff00ff"
                android:onClick="ck_st5b" />

        </LinearLayout>
        
        <!-- 7번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="0"
            android:orientation="horizontal" >

            <Button
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="11"
                android:background="#00ee0000" />
        </LinearLayout>
        
        <!-- 8번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="65"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_st5c"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="30"
                android:background="#00ff00ff"
                android:onClick="ck_st5c" />

        </LinearLayout>
        
        <!-- 9번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="0"
            android:orientation="horizontal" >

            <Button
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="11"
                android:background="#00ee0000" />
        </LinearLayout>
        
        <!-- 10번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="65"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_st5d"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="30"
                android:background="#00ff00ff"
                android:onClick="ck_st5d" />

        </LinearLayout>
        
        <!-- 11번째 -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="290"
            android:orientation="horizontal" >
            
            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:background="@drawable/lib_btm"
                />

        </LinearLayout>
        
    </LinearLayout>

</RelativeLayout>